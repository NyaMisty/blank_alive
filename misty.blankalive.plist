<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Label</key>
	<string>misty.blankalive</string>
	<key>ProgramArguments</key>
	<array>
		<string>/bin/sh</string>
		<string>/var/mobile/blankscreen/input_alive.sh</string>
	</array>
	<key>RunAtLoad</key>
	<true/>
	<key>StandardOutPath</key>
	<string>/var/mobile/blankscreen/blank_alive.log</string>
	<key>StartInterval</key>
	<integer>30</integer>
	<key>WorkingDirectory</key>
	<string>/var/mobile/blankscreen</string>
</dict>
</plist>
